-- –¢–∞–±–ª–∏—Ü–∞ –¥–ª—è —Ö—Ä–∞–Ω–µ–Ω–∏—è —ç–ª–µ–º–µ–Ω—Ç–æ–≤ –∫–ª–∞–≤–∏—à
local keyElements = {}

-- –§—É–Ω–∫—Ü–∏—è –¥–ª—è –æ–±–Ω–æ–≤–ª–µ–Ω–∏—è –æ—Ç–æ–±—Ä–∞–∂–µ–Ω–∏—è –∫–ª–∞–≤–∏—à
local function updateKeysDisplay()
    -- –û—á–∏—â–∞–µ–º —Å—Ç–∞—Ä—ã–µ —ç–ª–µ–º–µ–Ω—Ç—ã
    for _, element in pairs(keyElements) do
        if element and element.Parent then
            element:Destroy()
        end
    end
    keyElements = {}
    
    -- –û—á–∏—â–∞–µ–º KeysList
    for _, child in pairs(KeysList:GetChildren()) do
        if child:IsA("Frame") then
            child:Destroy()
        end
    end
    
    -- –°–æ–∑–¥–∞–µ–º –Ω–æ–≤—ã–µ —ç–ª–µ–º–µ–Ω—Ç—ã
    for i, keyData in ipairs(keySettings) do
        local KeyElement = Instance.new("Frame")
        KeyElement.Name = "KeyElement_" .. i
        KeyElement.Size = UDim2.new(1, 0, 0, 60)
        KeyElement.Position = UDim2.new(0, 0, 0, (i-1)*65)
        KeyElement.BackgroundColor3 = Color3.fromRGB(35, 35, 35)
        KeyElement.Parent = KeysList
        
        local KeyCorner = Instance.new("UICorner")
        KeyCorner.CornerRadius = UDim.new(0, 6)
        KeyCorner.Parent = KeyElement
        
        local KeyInput = Instance.new("TextBox")
        KeyInput.Name = "KeyInput"
        KeyInput.Size = UDim2.new(0.3, -5, 0.5, 0)
        KeyInput.Position = UDim2.new(0, 5, 0.25, 0)
        KeyInput.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
        KeyInput.BorderSizePixel = 0
        KeyInput.Text = keyData.key
        KeyInput.TextColor3 = Color3.fromRGB(255, 255, 255)
        KeyInput.TextSize = 14
        KeyInput.PlaceholderText = "–ö–ª–∞–≤–∏—à–∞"
        KeyInput.Parent = KeyElement
        
        local KeyInputCorner = Instance.new("UICorner")
        KeyInputCorner.CornerRadius = UDim.new(0, 4)
        KeyInputCorner.Parent = KeyInput
        
        local DelayInput = Instance.new("TextBox")
        DelayInput.Name = "DelayInput"
        DelayInput.Size = UDim2.new(0.3, -5, 0.5, 0)
        DelayInput.Position = UDim2.new(0.35, 0, 0.25, 0)
        DelayInput.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
        DelayInput.BorderSizePixel = 0
        DelayInput.Text = tostring(keyData.delay)
        DelayInput.TextColor3 = Color3.fromRGB(255, 255, 255)
        DelayInput.TextSize = 14
        DelayInput.PlaceholderText = "–ó–∞–¥–µ—Ä–∂–∫–∞ (–º—Å)"
        DelayInput.Parent = KeyElement
        
        local DelayCorner = Instance.new("UICorner")
        DelayCorner.CornerRadius = UDim.new(0, 4)
        DelayCorner.Parent = DelayInput
        
        local DeleteButton = Instance.new("TextButton")
        DeleteButton.Name = "DeleteButton"
        DeleteButton.Size = UDim2.new(0.3, -10, 0.5, 0)
        DeleteButton.Position = UDim2.new(0.7, 0, 0.25, 0)
        DeleteButton.BackgroundColor3 = Color3.fromRGB(213, 7, 69)
        DeleteButton.BorderSizePixel = 0
        DeleteButton.Text = "–£–¥–∞–ª–∏—Ç—å"
        DeleteButton.TextColor3 = Color3.fromRGB(255, 255, 255)
        DeleteButton.TextSize = 12
        DeleteButton.Parent = KeyElement
        
        local DeleteCorner = Instance.new("UICorner")
        DeleteCorner.CornerRadius = UDim.new(0, 4)
        DeleteCorner.Parent = DeleteButton
        
        -- –°–æ—Ö—Ä–∞–Ω—è–µ–º —ç–ª–µ–º–µ–Ω—Ç
        keyElements[i] = KeyElement
        
        -- –§—É–Ω–∫—Ü–∏—è –æ–±–Ω–æ–≤–ª–µ–Ω–∏—è –¥–∞–Ω–Ω—ã—Ö
        KeyInput.FocusLost:Connect(function()
            if KeyInput.Text ~= "" then
                keySettings[i].key = KeyInput.Text
            end
        end)
        
        DelayInput.FocusLost:Connect(function()
            local num = tonumber(DelayInput.Text)
            if num then
                keySettings[i].delay = num
            end
        end)
        
        -- –§—É–Ω–∫—Ü–∏—è —É–¥–∞–ª–µ–Ω–∏—è
        DeleteButton.MouseButton1Click:Connect(function()
            if #keySettings > 1 then
                table.remove(keySettings, i)
                updateKeysDisplay()
            else
                DeleteButton.Text = "–ù–µ–ª—å–∑—è —É–¥–∞–ª–∏—Ç—å!"
                task.wait(1)
                DeleteButton.Text = "–£–¥–∞–ª–∏—Ç—å"
            end
        end)
    end
    
    -- –û–±–Ω–æ–≤–ª—è–µ–º —Ä–∞–∑–º–µ—Ä –∫–∞–Ω–≤–∞—Å–∞
    KeysList.CanvasSize = UDim2.new(0, 0, 0, #keySettings * 65)
end

-- –ò–Ω–∏—Ü–∏–∞–ª–∏–∑–∞—Ü–∏—è —Å–ø–∏—Å–∫–∞ –∫–ª–∞–≤–∏—à
table.insert(keySettings, {key = "A", delay = 100})
updateKeysDisplay()

-- –ù–∞—Å—Ç—Ä–æ–π–∫–∞ –≤—ã–ø–∞–¥–∞—é—â–µ–≥–æ —Å–ø–∏—Å–∫–∞ –¥–ª—è –∫–Ω–æ–ø–æ–∫ –º—ã—à–∏
local mouseButtons = {"–õ–µ–≤–∞—è", "–ü—Ä–∞–≤–∞—è", "–°—Ä–µ–¥–Ω—è—è"}
local currentButtonIndex = 1

ButtonDropdown.MouseButton1Click:Connect(function()
    currentButtonIndex = currentButtonIndex + 1
    if currentButtonIndex > #mouseButtons then
        currentButtonIndex = 1
    end
    ButtonDropdown.Text = mouseButtons[currentButtonIndex]
end)

-- –ó–∞—Ö–≤–∞—Ç –ø–æ–∑–∏—Ü–∏–∏ –º—ã—à–∏
CapturePosButton.MouseButton1Click:Connect(function()
    local x = math.floor(mouse.X)
    local y = math.floor(mouse.Y)
    
    XPosBox.Text = tostring(x)
    YPosBox.Text = tostring(y)
    
    CapturePosButton.Text = "–ü–æ–∑–∏—Ü–∏—è –∑–∞—Ö–≤–∞—á–µ–Ω–∞!"
    task.wait(1)
    CapturePosButton.Text = "–ó–∞—Ö–≤–∞—Ç–∏—Ç—å –ø–æ–∑–∏—Ü–∏—é –º—ã—à–∏"
end)

-- –ü–µ—Ä–µ–∫–ª—é—á–µ–Ω–∏–µ –≤–∫–ª–∞–¥–æ–∫
MouseTab.MouseButton1Click:Connect(function()
    MouseSettings.Visible = true
    KeysSettings.Visible = false
    
    MouseTab.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
    KeysTab.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
end)

KeysTab.MouseButton1Click:Connect(function()
    MouseSettings.Visible = false
    KeysSettings.Visible = true
    
    MouseTab.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
    KeysTab.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
end)

-- –§—É–Ω–∫—Ü–∏—è –¥–ª—è –∫–ª–∏–∫–∞ –º—ã—à—å—é
local function performMouseClick(x, y, button)
    local buttonCode
    if button == "–õ–µ–≤–∞—è" then
        buttonCode = 0
    elseif button == "–ü—Ä–∞–≤–∞—è" then
        buttonCode = 1
    elseif button == "–°—Ä–µ–¥–Ω—è—è" then
        buttonCode = 2
    else
        buttonCode = 0
    end
    
    VirtualInputManager:SendMouseButtonEvent(x, y, buttonCode, true, game, 1)
    task.wait(0.01)
    VirtualInputManager:SendMouseButtonEvent(x, y, buttonCode, false, game, 1)
end

-- –§—É–Ω–∫—Ü–∏—è –¥–ª—è –Ω–∞–∂–∞—Ç–∏—è –∫–ª–∞–≤–∏—à–∏
local function pressKey(key)
    local keyCode = Enum.KeyCode[key]
    if keyCode then
        VirtualInputManager:SendKeyEvent(true, keyCode, false, game)
        task.wait(0.01)
        VirtualInputManager:SendKeyEvent(false, keyCode, false, game)
    end
end

-- –ê–≤—Ç–æ-–∫–ª–∏–∫–µ—Ä –º—ã—à—å—é
StartMouseButton.MouseButton1Click:Connect(function()
    clickerEnabled = not clickerEnabled
    
    if clickerEnabled then
        StartMouseButton.Text = "–í–´–ö–õ–Æ–ß–ò–¢–¨"
        StartMouseButton.BackgroundColor3 = Color3.fromRGB(213, 7, 69)
        
        local speed = tonumber(SpeedBox.Text) or 100
        local x = tonumber(XPosBox.Text) or 960
        local y = tonumber(YPosBox.Text) or 540
        local button = ButtonDropdown.Text
        
        local connection
        connection = RunService.Heartbeat:Connect(function()
            if not clickerEnabled then
                connection:Disconnect()
                return
            end
            
            performMouseClick(x, y, button)
            task.wait(speed / 1000)
        end)
        
        table.insert(connections, connection)
    else
        StartMouseButton.Text = "–í–ö–õ–Æ–ß–ò–¢–¨"
        StartMouseButton.BackgroundColor3 = Color3.fromRGB(253, 104, 159)
    end
end)

-- –ê–≤—Ç–æ-–Ω–∞–∂–∞—Ç–∏–µ –∫–ª–∞–≤–∏—à
StartKeysButton.MouseButton1Click:Connect(function()
    keyPresserEnabled = not keyPresserEnabled
    
    if keyPresserEnabled then
        StartKeysButton.Text = "–í–´–ö–õ–Æ–ß–ò–¢–¨"
        StartKeysButton.BackgroundColor3 = Color3.fromRGB(213, 7, 69)
        
        local keyIndex = 1
        local lastPressTime = os.clock()
        
        local connection
        connection = RunService.Heartbeat:Connect(function()
            if not keyPresserEnabled then
                connection:Disconnect()
                return
            end
            
            -- –ï—Å–ª–∏ —Å–ø–∏—Å–æ–∫ –ø—É—Å—Ç, –Ω–∏—á–µ–≥–æ –Ω–µ –¥–µ–ª–∞–µ–º
            if #keySettings == 0 then
                return
            end
            
            local currentTime = os.clock()
            if currentTime - lastPressTime >= (keySettings[keyIndex].delay / 1000) then
                pressKey(keySettings[keyIndex].key)
                keyIndex = keyIndex + 1
                if keyIndex > #keySettings then
                    keyIndex = 1
                end
                lastPressTime = currentTime
            end
        end)
        
        table.insert(connections, connection)
    else
        StartKeysButton.Text = "–í–ö–õ–Æ–ß–ò–¢–¨"
        StartKeysButton.BackgroundColor3 = Color3.fromRGB(253, 104, 159)
    end
end)

-- –î–æ–±–∞–≤–ª–µ–Ω–∏–µ –Ω–æ–≤–æ–π –∫–ª–∞–≤–∏—à–∏
AddKeyButton.MouseButton1Click:Connect(function()
    table.insert(keySettings, {key = "B", delay = 200})
    updateKeysDisplay()
end)

-- –ü–µ—Ä–µ—Ç–∞—Å–∫–∏–≤–∞–Ω–∏–µ –æ–∫–Ω–∞
local dragging = false
local dragInput, dragStart, startPos

local function updateInput(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 then
        dragging = true
        dragStart = input.Position
        startPos = MainFrame.Position
        
        input.Changed:Connect(function()
            if input.UserInputState == Enum.UserInputState.End then
                dragging = false
            end
        end)
    end
end

local function updateFrame(input)
    if dragging then
        local delta = input.Position - dragStart
        MainFrame.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
    end
end

TopBar.InputBegan:Connect(updateInput)
TopBar.InputChanged:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseMovement then
        updateFrame(input)
    end
end)

-- –°–≤–æ—Ä–∞—á–∏–≤–∞–Ω–∏–µ –æ–∫–Ω–∞
local minimized = false
MinimizeButton.MouseButton1Click:Connect(function()
    minimized = not minimized
    
    if minimized then
        MainFrame:TweenSize(UDim2.new(0, 394, 0, 50), "Out", "Quad", 0.3, true)
        MinimizeButton.Text = "+"
    else
        MainFrame:TweenSize(UDim2.new(0, 394, 0, 331), "Out", "Quad", 0.3, true)
        MinimizeButton.Text = "-"
    end
end)

-- –ó–∞–∫—Ä—ã—Ç–∏–µ –æ–∫–Ω–∞
CloseButton.MouseButton1Click:Connect(function()
    clickerEnabled = false
    keyPresserEnabled = false
    
    for _, connection in ipairs(connections) do
        connection:Disconnect()
    end
    
    ScreenGui:Destroy()
end)

