        local jsonData = game:GetService("HttpService"):JSONDecode(json)
        local url = jsonData[tostring(game.PlaceId)] or "universal.lua"
        url = url.s or url
        Functions.LoadScript("https://lkhub.net/s/"..url)
    end
})

