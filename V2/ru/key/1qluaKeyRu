    Title = "Loop Menu",
    Description = "Редактировать поле зрения + скорость + прыжок",
    Callback = function()
        loadstring(game:HttpGet(('https://raw.githubusercontent.com/1Qlua-RBXscript/1qlua/refs/heads/main/Menu/Speed'),true))()
    end
})


