-- Функция получения списка игроков
function GetPlayerNames()
    local players = {}
    local success, result = pcall(function()
        for _, player in ipairs(game:GetService("Players"):GetPlayers()) do
            if player ~= game:GetService("Players").LocalPlayer then
                table.insert(players, player.Name)
            end
        end
    end)
    return players
end

-- Функция получения валидных игроков
function GetValidPlayers()
    local validPlayers = {}
    local localPlayer = game:GetService("Players").LocalPlayer
    
    for _, player in ipairs(game:GetService("Players"):GetPlayers()) do
        if player ~= localPlayer and IsValidTarget(player) then
            table.insert(validPlayers, player)
        end
    end
    
    return validPlayers
end

-- Функция проверки валидности цели
function IsValidTarget(player)
    if not player then return false end
    
    -- Проверка "только друзья"
    if Teleport.FriendsOnly then
        local success, isFriend = pcall(function()
            return player:IsFriendsWith(game:GetService("Players").LocalPlayer.UserId)
        end)
        if not success or not isFriend then
            return false
        end
    end
    
    -- Проверка на мертвых игроков
    if Teleport.SkipDead then
        local character = player.Character
        if character then
            local humanoid = character:FindFirstChildOfClass("Humanoid")
            if humanoid and humanoid.Health <= 0 then
                return false
            end
        end
    end
    
    return true
end

-- Функция телепортации к игроку
function TeleportToPlayer(player)
    if not player then return false end
    
    local success, err = pcall(function()
        local localPlayer = game:GetService("Players").LocalPlayer
        local localCharacter = localPlayer.Character
        local targetCharacter = player.Character
        
        if not localCharacter or not targetCharacter then 
            return false
        end
        
        local targetPart = targetCharacter:FindFirstChild(Teleport.BodyPart) or targetCharacter:FindFirstChild("HumanoidRootPart")
        local localRoot = localCharacter:FindFirstChild("HumanoidRootPart")
        
        if not targetPart or not localRoot then 
            return false
        end
        
        -- Вычисляем позицию с учетом расстояния
        local lookVector = targetPart.CFrame.LookVector
        local teleportPosition = targetPart.Position - (lookVector * Teleport.DistanceOffset)
        local newCFrame = CFrame.new(teleportPosition + Vector3.new(0, Teleport.HeightOffset, 0))
        
        -- Телепортируемся
        localRoot.CFrame = newCFrame
        
        -- Показываем информацию об игроке если включено
        if Teleport.ShowInfo then
            ShowPlayerInfo(player)
        end
        
        return true
    end)
    
    if not success then
        warn("Ошибка телепортации: " .. tostring(err))
        return false
    end
    
    return true
end

-- Функция показа информации об игроке
function ShowPlayerInfo(player)
    local character = player.Character
    local info = "Телепортирован к: " .. player.Name
    
    if character then
        local humanoid = character:FindFirstChildOfClass("Humanoid")
        if humanoid then
            info = info .. "\nЗдоровье: " .. math.floor(humanoid.Health) .. "/" .. math.floor(humanoid.MaxHealth)
        end
    end
    
    Notifier.new({
        Title = "Информация о телепортации",
        Content = info,
        Duration = 2,
        Icon = "rbxassetid://120245531583106"
    })
end

-- Функция получения следующей цели
function GetNextTarget(players)
    if #players == 0 then return nil end
    
    if Teleport.Mode == "По порядку" then
        local target = players[Teleport.CurrentIndex]
        Teleport.CurrentIndex = Teleport.CurrentIndex % #players + 1
        return target
    elseif Teleport.Mode == "Случайно" then
        return players[math.random(1, #players)]
    elseif Teleport.Mode == "Ближайший" then
        return GetNearestPlayer(players)
    elseif Teleport.Mode == "Самый дальний" then
        return GetFurthestPlayer(players)
    else
        return players[1]
    end
end

-- Функция получения ближайшего игрока
function GetNearestPlayer(players)
    local localPlayer = game:GetService("Players").LocalPlayer
    local localCharacter = localPlayer.Character
    if not localCharacter then return players[1] end
    
    local localRoot = localCharacter:FindFirstChild("HumanoidRootPart")
    if not localRoot then return players[1] end
    
    local nearestPlayer = players[1]
    local nearestDistance = math.huge
    
    for _, player in ipairs(players) do
        local character = player.Character
        if character then
            local root = character:FindFirstChild("HumanoidRootPart")
            if root then
                local distance = (localRoot.Position - root.Position).Magnitude
                if distance < nearestDistance then
                    nearestDistance = distance
                    nearestPlayer = player
                end
            end
        end
    end
    
    return nearestPlayer
end

-- Функция получения самого дальнего игрока
function GetFurthestPlayer(players)
    local localPlayer = game:GetService("Players").LocalPlayer
    local localCharacter = localPlayer.Character
    if not localCharacter then return players[1] end
    
    local localRoot = localCharacter:FindFirstChild("HumanoidRootPart")
    if not localRoot then return players[1] end
    
    local furthestPlayer = players[1]
    local furthestDistance = 0
    
    for _, player in ipairs(players) do
        local character = player.Character
        if character then
            local root = character:FindFirstChild("HumanoidRootPart")
            if root then
                local distance = (localRoot.Position - root.Position).Magnitude
                if distance > furthestDistance then
                    furthestDistance = distance
                    furthestPlayer = player
                end
            end
        end
    end
    
    return furthestPlayer
end

-- Функция старта автоматической телепортации (исправленная)
function StartAutoTeleport()
    if Teleport.Enabled then return end
    
    Teleport.Enabled = true
    Teleport.CurrentIndex = 1
    Teleport.LastTeleportTime = 0
    
    Notifier.new({
        Title = "Телепортация",
        Content = "Авто-телепортация запущена!",
        Duration = 3,
        Icon = "rbxassetid://120245531583106"
    })
    
    -- Создаем отдельный поток для телепортации с правильным таймером
    task.spawn(function()
        while Teleport.Enabled do
            local players = GetValidPlayers()
            if #players > 0 then
                local targetPlayer = GetNextTarget(players)
                if targetPlayer then
                    local success = TeleportToPlayer(targetPlayer)
                    if success then
                        print("Успешно телепортирован к: " .. targetPlayer.Name)
                    end
                end
            else
                print("Нет подходящих игроков для телепортации")
            end
            
            -- Ждем указанное количество секунд
            wait(Teleport.Speed)
        end
    end)
end

-- Функция остановки автоматической телепортации
function StopAutoTeleport()
    if not Teleport.Enabled then return end
    
    Teleport.Enabled = false
    
    Notifier.new({
        Title = "Телепортация",
        Content = "Авто-телепортация остановлена!",
        Duration = 3,
        Icon = "rbxassetid://120245531583106"
    })
end

-- Функция ручной телепортации к следующему игроку
function TeleportToNextPlayerManual()
    if Teleport.Enabled then
        Notifier.new({
            Title = "Телепортация",
            Content = "Сначала отключите авто-телепортацию!",
            Duration = 3,
            Icon = "rbxassetid://120245531583106"
        })
        return
    end
    
    local players = GetValidPlayers()
    if #players > 0 then
        local targetPlayer = GetNextTarget(players)
        if targetPlayer and TeleportToPlayer(targetPlayer) then
            Notifier.new({
                Title = "Телепортация",
                Content = "Успешно телепортирован к: " .. targetPlayer.Name,
                Duration = 3,
                Icon = "rbxassetid://120245531583106"
            })
        else
            Notifier.new({
                Title = "Телепортация",
                Content = "Не удалось телепортироваться к игроку!",
                Duration = 3,
                Icon = "rbxassetid://120245531583106"
            })
        end
    else
        Notifier.new({
            Title = "Телепортация",
            Content = "Не найдено подходящих игроков!",
            Duration = 3,
            Icon = "rbxassetid://120245531583106"
        })
    end
end

-- Функция обновления списка игроков
function RefreshPlayerList()
    local playerCount = #GetValidPlayers()
    Notifier.new({
        Title = "Список Игроков",
        Content = "Список игроков обновлен! Доступно: " .. playerCount,
        Duration = 3,
        Icon = "rbxassetid://120245531583106"
    })
end

-- Функция для безопасного завершения
local function Cleanup()
    Teleport.Enabled = false
end

-- Очистка при выходе из игры
game:GetService("Players").LocalPlayer.OnTeleport:Connect(Cleanup)

-- Секция копирователя путей объектов
local MainSection = PlayerTab:DrawSection({
    Name = "Копирователь путей объектов",
    Position = 'left'
})

-- Переменные
local pathCopierEnabled = false
local currentPath = ""
local pathCopierConnection = nil

-- Функция получения пути через GetChildren
local function getPathWithGetChildren(object)
    if not object or not object.Parent then
        return "Неверный объект"
    end
    
    local pathParts = {}
    local current = object
    
    while current and current ~= game do
        local parent = current.Parent
        if parent then
            local children = parent:GetChildren()
            for index, child in ipairs(children) do
                if child == current then
                    table.insert(pathParts, 1, {name = current.Name, index = index})
                    break
                end
            end
            current = parent
        else
            break
        end
    end
    
    local result = "game"
    for i, part in ipairs(pathParts) do
        if i == 1 then
            result = result .. ":GetService(\"" .. part.name .. "\")"
        else
            result = result .. ":GetChildren()[" .. part.index .. "]"
        end
    end
    
    return result
end

-- Функция получения полного пути
local function getFullPath(object)
    if not object then 
        return "Неверный объект" 
    end
    local success, result = pcall(function()
        return object:GetFullName()
    end)
    if success then
        return result
    else
        return "Ошибка получения пути"
    end
end

-- Функция получения Debug пути
local function getDebugPath(object)
    if not object then 
        return "Неверный объект" 
    end
    local fullName = getFullPath(object)
    local objectId = tostring(object:GetDebugId())
    local className = object.ClassName
    return fullName .. " -- DebugId: " .. objectId .. " Class: " .. className
end

-- Основная функция получения пути
local function getSmartPath(object)
    local format = getgenv().PathCopierFormat or "GetChildren"
    if format == "GetChildren" then
        return getPathWithGetChildren(object)
    elseif format == "FullPath" then
        return getFullPath(object)
    elseif format == "Debug" then
        return getDebugPath(object)
    else
        return getPathWithGetChildren(object)
    end
end

-- Функция подсветки объекта
local function highlightObject(object)
    if not object then 
        return 
    end
    if not getgenv().PathCopierHighlight then 
        return 
    end
    
    local oldHighlight = object:FindFirstChild("PathCopierHighlight")
    if oldHighlight then
        oldHighlight:Destroy()
    end
    
    local highlight = Instance.new("Highlight")
    highlight.Name = "PathCopierHighlight"
    highlight.Adornee = object
    highlight.FillColor = getgenv().PathCopierHighlightColor or Color3.fromRGB(255, 255, 0)
    highlight.FillTransparency = 0.5
    highlight.OutlineColor = Color3.fromRGB(255, 255, 255)
    highlight.OutlineTransparency = 0
    highlight.Parent = object
    
    delay(3, function()
        if highlight and highlight.Parent then
            highlight:Destroy()
        end
    end)
end

-- Функция обработки клика по объекту
local function onObjectClicked(object)
    if not pathCopierEnabled or not object then 
        return 
    end
    
    local path = getSmartPath(object)
    currentPath = path
    highlightObject(object)
    
    pcall(function()
        setclipboard(path)
    end)
    
    if Window and Window.Notify then
        Window.Notify.new({
            Title = "Путь скопирован",
            Content = "Объект: " .. object.Name .. "\nКласс: " .. object.ClassName .. "\nПуть скопирован!",
            Duration = 4,
            Icon = "copy"
        })
    end
    
    print("[Path Copier] Объект: " .. object.Name .. " | Путь: " .. path)
end

-- Функция настройки копирователя
local function setupPathCopier()
    if pathCopierConnection then
        pathCopierConnection:Disconnect()
    end
    
    local player = game:GetService("Players").LocalPlayer
    local mouse = player:GetMouse()
    
    pathCopierConnection = mouse.Button1Down:Connect(function()
        local target = mouse.Target
        if target and pathCopierEnabled then
            wait(0.01)
            if target.Parent then
                onObjectClicked(target)
            end
        end
    end)
end

-- Функция отключения копирователя
local function teardownPathCopier()
    if pathCopierConnection then
        pathCopierConnection:Disconnect()
        pathCopierConnection = nil
    end
end

-- Инициализация настроек
if not getgenv().PathCopierFormat then
    getgenv().PathCopierFormat = "GetChildren"
end
if getgenv().PathCopierHighlight == nil then
    getgenv().PathCopierHighlight = true
end
if not getgenv().PathCopierHighlightColor then
    getgenv().PathCopierHighlightColor = Color3.fromRGB(255, 255, 0)
end

-- Toggle для включения/выключения
local PathCopierToggle = MainSection:AddToggle({
    Name = "Включить \nкопирование ",
    Flag = "PathCopier_Enabled",
    Default = false,
    Callback = function(enabled)
        pathCopierEnabled = enabled
        if enabled then
            if Window and Window.Notify then
                Window.Notify.new({
                    Title = "Копирователь путей",
                    Content = "Инструмент включен! Нажмите на любой объект чтобы скопировать его путь.",
                    Duration = 3,
                    Icon = "copy"
                })
            end
            setupPathCopier()
        else
            teardownPathCopier()
            if Window and Window.Notify then
                Window.Notify.new({
                    Title = "Копирователь путей",
                    Content = "Инструмент выключен.",
                    Duration = 2,
                    Icon = "copy"
                })
            end
        end
    end
})

-- Настройка формата пути
local PathFormatDropdown = MainSection:AddDropdown({
    Name = "Формат пути",
    Default = "GetChildren",
    Flag = "PathCopier_Format",
    Values = {"GetChildren", "FullPath", "Debug"},
    Callback = function(format)
        getgenv().PathCopierFormat = format
    end
})

-- Настройка подсветки
local HighlightToggle = MainSection:AddToggle({
    Name = "Включить подсветку",
    Flag = "PathCopier_Highlight",
    Default = true,
    Callback = function(enabled)
        getgenv().PathCopierHighlight = enabled
    end
})

-- Цвет подсветки
local HighlightColor = MainSection:AddColorPicker({
    Name = "Цвет подсветки",
    Default = Color3.fromRGB(255, 255, 0),
    Flag = "PathCopier_HighlightColor",
    Callback = function(color)
        getgenv().PathCopierHighlightColor = color
    end
})

-- Кнопка копирования
local CopyButton = MainSection:AddButton({
    Name = "Скопировать последний путь",
    Callback = function()
        if currentPath ~= "" then
            pcall(function()
                setclipboard(currentPath)
            end)
            if Window and Window.Notify then
                Window.Notify.new({
                    Title = "Копирователь путей",
                    Content = "Путь скопирован в буфер обмена!",
                    Duration = 2,
                    Icon = "check"
                })
            end
        else
            if Window and Window.Notify then
                Window.Notify.new({
                    Title = "Копирователь путей",
                    Content = "Нет пути для копирования!",
                    Duration = 2,
                    Icon = "warning"
                })
            end
        end
    end
})

-- Инструкция
MainSection:AddParagraph({
    Title = "Форматы путей",
    Content = "GetChildren - путь через индексы детей\nFullPath - обычный полный путь\nDebug - с ID объекта и классом"
})

-- Автоотключение при выходе
game:GetService("Players").LocalPlayer:GetPropertyChangedSignal("Parent"):Connect(function()
    if not game:GetService("Players").LocalPlayer.Parent then
        teardownPathCopier()
    end
end)



PlayerScriptsSection:AddToggle({
    Name = "Anti-Lag",
    Flag = "AntiLag",
    Default = false,
    Callback = function(state)
        antiLagEnabled = state
        if state then
            local Terrain = workspace:FindFirstChildOfClass('Terrain')
            Terrain.WaterWaveSize = 0
            Terrain.WaterWaveSpeed = 0
            Terrain.WaterReflectance = 0
            Terrain.WaterTransparency = 1
            
            Lighting.GlobalShadows = false
            Lighting.FogEnd = 9e9
            Lighting.FogStart = 9e9
            settings().Rendering.QualityLevel = 1
            
            for _,v in pairs(game:GetDescendants()) do
                if v:IsA("BasePart") then
                    v.Material = "Plastic"
                    v.Reflectance = 0
                    for _, surface in pairs({"Back", "Bottom", "Front", "Left", "Right", "Top"}) do
                        v[surface.."Surface"] = "SmoothNoOutlines"
                    end
                elseif v:IsA("Decal") then
                    v.Transparency = 1
                elseif v:IsA("ParticleEmitter") or v:IsA("Trail") then
                    v.Lifetime = NumberRange.new(0)
                end
            end
            
            for _,v in pairs(Lighting:GetDescendants()) do
                if v:IsA("PostEffect") then
                    v.Enabled = false
                end
            end
            
            workspace.DescendantAdded:Connect(function(child)
                if child:IsA('ForceField') or child:IsA('Sparkles') or child:IsA('Smoke') or child:IsA('Fire') or child:IsA('Beam') then
                    task.wait()
                    child:Destroy()
                end
            end)
            
            notify("Anti-Lag", "Enabled")
        else
            notify("Anti-Lag", "Restart game to disable")
        end
    end
})

PlayerScriptsSection:AddToggle({
    Name = "Noclip",
    Flag = "NoclipToggle",
    Default = false,
    Callback = function(state)
        if state then
            Clip = false
            local function NoclipLoop()
                if Clip == false and speaker.Character ~= nil then
                    for _, child in pairs(speaker.Character:GetDescendants()) do
                        if child:IsA("BasePart") and child.CanCollide == true then
                            child.CanCollide = false
                        end
                    end
                end
            end
            Noclipping = RunService.Stepped:Connect(NoclipLoop)
            notify('Noclip', 'Enabled')
        else
            if Noclipping then
                Noclipping:Disconnect()
                Noclipping = nil
            end
            Clip = true
            notify('Noclip', 'Disabled')
        end
    end
})

PlayerScriptsSection:AddButton({
    Name = "Запускает нужные скрипты",
    Description = "Запускает парочку нужных скриптов для игры",
    Callback = function()
        Functions.LoadScript("https://raw.githubusercontent.com/1Qlua-RBXscript/1qlua/refs/heads/main/ALLSCRIPTRUN")
    end
