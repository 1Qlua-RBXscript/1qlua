
local scripts = {
    "https://raw.githubusercontent.com/1Qlua-RBXscript/1qlua/refs/heads/main/AIBOT/Kat/A",
    "https://raw.githubusercontent.com/1Qlua-RBXscript/1qlua/refs/heads/main/AIBOT/Kat/Revolver",
    "https://raw.githubusercontent.com/1Qlua-RBXscript/1qlua/refs/heads/main/AIBOT/Kat/kamera",
    "https://raw.githubusercontent.com/1Qlua-RBXscript/1qlua/refs/heads/main/AIBOT/Kat/prizok",
    "https://raw.githubusercontent.com/1Qlua-RBXscript/1qlua/refs/heads/main/AIBOT/Kat/DELGUI",
    "https://raw.githubusercontent.com/1Qlua-RBXscript/1qlua/refs/heads/main/AIBOT/Kat/walk"
}

for i, url in ipairs(scripts) do
    coroutine.wrap(function()
        local success, err = pcall(function()
            local scriptContent = game:HttpGet(url)
            loadstring(scriptContent)()
        end)
        
        if not success then
            warn("Ошибка в скрипте "..i.." ("..url.."): "..err)
        else
            print("Скрипт "..i.." успешно запущен: "..url)
        end
    end)()
end
