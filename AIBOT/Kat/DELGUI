-- –ü–†–û–°–¢–û–ô –ò –≠–§–§–ï–ö–¢–ò–í–ù–´–ô –°–ö–†–ò–ü–¢
local Players = game:GetService("Players")
local CoreGui = game:GetService("CoreGui")

wait(2) -- –ñ–¥–µ–º –∑–∞–≥—Ä—É–∑–∫–∏

local player = Players.LocalPlayer
if player then
    -- –£–¥–∞–ª—è–µ–º –≤—Å–µ —á—Ç–æ –Ω–µ —è–≤–ª—è–µ—Ç—Å—è –∏–≥—Ä–æ–≤—ã–º —ç–ª–µ–º–µ–Ω—Ç–æ–º
    for _, gui in pairs(player.PlayerGui:GetDescendants()) do
        local name = gui.Name:lower()
        
        -- –£–¥–∞–ª—è–µ–º –ª–∏–¥–µ—Ä–±–æ—Ä–¥—ã, –Ω–∞—Å—Ç—Ä–æ–π–∫–∏, —á–∞—Ç
        if name:find("leader") or name:find("stat") or 
           name:find("chat") or name:find("message") or
           name:find("setting") or name:find("menu") or
           name:find("shop") or name:find("button") then
            
            pcall(function()
                gui.Visible = false
                gui:Destroy()
            end)
        end
    end
end

-- –£–¥–∞–ª—è–µ–º —Å–∏—Å—Ç–µ–º–Ω—ã–µ —ç–ª–µ–º–µ–Ω—Ç—ã Roblox
local systemElements = {"SettingsShield", "Chat", "PlayerList", "PurchasePrompt"}
for _, element in pairs(systemElements) do
    local elem = CoreGui:FindFirstChild(element)
    if elem then
        elem:Destroy()
    end
end

print("üóëÔ∏è –£–¥–∞–ª–µ–Ω—ã –≤—Å–µ –Ω–µ–Ω—É–∂–Ω—ã–µ —ç–ª–µ–º–µ–Ω—Ç—ã!")
